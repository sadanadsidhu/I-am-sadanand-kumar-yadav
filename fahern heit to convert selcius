import java.util.Scanner;
public class FahernheitToSelcius {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int s=sc.nextInt();
		int e=sc.nextInt();
		int w=sc.nextInt();
		int fah;
		while(s<=e){
			fah=((5*(s-32))/9);
			System.out.println(s+" "+fah);
			s+=w;
					
		}
		
		}

}
